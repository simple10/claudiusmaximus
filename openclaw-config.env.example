# VPS Configuration (required)
VPS1_IP=15.x.x.1
VPS1_HOSTNAME=openclaw

# SSH Configuration (required)
SSH_KEY_PATH=~/.ssh/ovh_openclaw_ed25519
SSH_USER=ubuntu # This will get changed to adminclaw during hardening
SSH_PORT=22 # This will get changed to 222 during hardening

# Networking Option (required)
# - "cloudflare-tunnel" (Recommended) - Zero exposed ports, origin IP hidden
# - "caddy" - Port 443 exposed, uses Cloudflare Origin CA
NETWORKING_OPTION=cloudflare-tunnel

# Domain Configuration (required for networking)
DOMAIN_OPENCLAW=openclaw.example.com

# URL subpaths for obscured access (no trailing slash)
# Set to empty string "" to serve at root (e.g., when using dedicated domains with Cloudflare Tunnel)
# If using caddy network option, it's recommended to set subpaths to protect against bot scanners
# If using Cloudflare Tunnel, it's recommended to serve openclaw from root (no subpath)
SUBPATH_OPENCLAW= # serve from root

# API Keys (required)
# OpenClaw needs at least one provider key to start the onboard process
# Anthropic is the recommended provider by OpenClaw
# During onboarding, additional providers can be configured
ANTHROPIC_API_KEY=sk-123

# AI Gateway Worker (optional — routes LLM requests through Cloudflare AI Gateway)
# AI_GATEWAY_WORKER_URL=https://ai-gateway-proxy.<account>.workers.dev
# AI_GATEWAY_AUTH_TOKEN=<worker-auth-token>

# Log Receiver Worker (optional — ships container logs to Cloudflare)
# LOG_WORKER_URL=https://log-receiver.<account>.workers.dev/logs
# LOG_WORKER_TOKEN=<generated-token>

# Host Alerter (optional — disk/memory/CPU alerts via Telegram)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Messaging Channels (optional - leave empty if not using)
DISCORD_BOT_TOKEN=
SLACK_BOT_TOKEN=
SLACK_APP_TOKEN=
