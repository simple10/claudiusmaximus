# VPS Configuration (required)
VPS1_IP=15.x.x.1
VPS1_HOSTNAME=openclaw

# SSH Configuration (required)
SSH_KEY_PATH=~/.ssh/ovh_openclaw_ed25519
SSH_USER=ubuntu # This will get changed to adminclaw during hardening
SSH_PORT=22 # This will get changed to 222 during hardening

# Domain Configuration (required)
OPENCLAW_DOMAIN=openclaw.example.com

# URL subpaths for obscured access (no trailing slash)
# Set to empty string "" to serve at root (e.g., when using dedicated domains with Cloudflare Tunnel)
OPENCLAW_DOMAIN_PATH= # serve from root

# AI Gateway (required — routes ALL LLM requests through Cloudflare AI Gateway)
# Real provider API keys live only on the Worker (Cloudflare), never on the VPS.
AI_GATEWAY_WORKER_URL=https://ai-gateway-proxy.<account>.workers.dev
AI_GATEWAY_AUTH_TOKEN=<worker-auth-token>

# Log Receiver Worker (optional — ships container logs to Cloudflare)
# LOG_WORKER_URL=https://log-receiver.<account>.workers.dev/logs
# LOG_WORKER_TOKEN=<generated-token>

# Host Alerter (optional — disk/memory/CPU alerts via Telegram)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Messaging Channels (optional - leave empty if not using)
DISCORD_BOT_TOKEN=
SLACK_BOT_TOKEN=
SLACK_APP_TOKEN=
